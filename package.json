{
  "name": "medium-draft",
  "version": "2.0.0-next",
  "description": "A medium like rich text editor built upon draft-js with an emphasis on eliminating mouse usage by adding relevant keyboard shortcuts",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/cjs/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --host=0.0.0.0 --port=8080 --hot --inline",
    "dist": "webpack --mode production",
    "tslint": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "build:esm": "tsc --declaration --outDir ./dist/esm --target es6 --module es6",
    "build": "tsc --declaration && npm run build:esm && npm run dist",
    "local": "mkdir -p local && cp -r lib dist package.json local",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumistoklus/medium-draft.git"
  },
  "types": "./dist/main.d.ts",
  "keywords": [],
  "author": "Brijesh Bittu <brijesh@bitwiser.in> (http://bitwiser.in/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumistoklus/medium-draft/issues"
  },
  "homepage": "https://github.conode-sassm/dumistoklus/medium-draft#readme",
  "devDependencies": {
    "@types/draft-js": "0.10.32",
    "@types/memoize-one": "4.1.1",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/webpack-env": "1.13.9",
    "autoprefixer": "^9.6.0",
    "css-loader": "2.1.1",
    "draft-js": "0.10.5",
    "draft-js-export-html": "^1.3.3",
    "html-webpack-plugin": "3.2.0",
    "memoize-one": "5.0.4",
    "mini-css-extract-plugin": "0.7.0",
    "postcss-loader": "3.0.0",
    "postcss-nesting": "^7.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "style-loader": "0.23.1",
    "ts-loader": "6.0.2",
    "tslint": "^5.17.0",
    "typescript": "3.5.1",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.3",
    "webpack-dev-server": "3.7.1"
  },
  "dependencies": {
    "memoize-one": "5.0.4"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "draft-js": "0.10.5"
  }
}
